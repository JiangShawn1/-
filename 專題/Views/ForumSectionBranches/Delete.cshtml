@model 專題.Models.EFModels.ForumSectionBranch

@{
    ViewBag.Title = "Delete";
}

<h2>確認刪除</h2>

<h3>確定要刪除這個區塊嗎</h3>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt>
           區塊名稱
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ForumSection.sectionName)
        </dd>

        <dt>
           分支管理者
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Member.Name)
        </dd>

        <dt>
          分支名稱
        </dt>

        <dd>
            @Html.DisplayFor(model => model.branchName)
        </dd>

        <dt>
            區塊管理者
        </dt>

        <dd>
            @Html.DisplayFor(model => model.sectionAdministrator)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-default" /> |
            @Html.ActionLink("回到分支管理", "Index")
        </div>
    }
</div>
