@model IEnumerable<專題.Models.EFModels.ForumSectionBranch>

<p>
    @Html.ActionLink("新增分支", "Create")
</p>
<table class="table">
    <tr>
        <th>
            區塊名稱
        </th>
        <th>
            管理者
        </th>
        <th>
           分支名稱
        </th>
    
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ForumSection.sectionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Member.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.branchName)
        </td>
       
        <td>
            @Html.ActionLink("修改", "Edit", new { id=item.id }) |
            @Html.ActionLink("詳情", "Details", new { id=item.id }) |
            @Html.ActionLink("刪除", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
